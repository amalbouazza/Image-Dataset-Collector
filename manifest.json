{
    "manifest_version": 3,
    "name": "Image Dataset Collector",
    "version": "1.0",
    "description": "Extension pour ajouter des images à un dataset avec des informations supplémentaires.",
    "permissions": [
      "contextMenus",
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/camera-16.png",
        "64": "icons/camera-64.png",
        "128": "icons/camera-128.png"
      }
    },
    "icons": {
     "16": "icons/camera-16.png",
        "64": "icons/camera-64.png",
        "128": "icons/camera-128.png"
    }
  }